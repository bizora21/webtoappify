# Environment variables for Appwrite backend integration

# Appwrite endpoint (e.g., https://cloud.appwrite.io/v1)
APPWRITE_ENDPOINT=https://YOUR_APPWRITE_ENDPOINT/v1

# Your Appwrite project ID (found in the Appwrite console)
APPWRITE_PROJECT_ID=YOUR_PROJECT_ID

# Secret API key with full permissions (service key)
APPWRITE_API_KEY=YOUR_API_KEY

# Database ID where the "builds" collection will live
APPWRITE_DATABASE_ID=YOUR_DATABASE_ID

# Collection ID for builds (create in Appwrite console)
APPWRITE_COLLECTION_ID=YOUR_COLLECTION_ID
